@model IEnumerable<Notification>
@{
    ViewData["Title"] = "Notifications";
}

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 style="color:#007AA3">Messages</h2>
        </div>
    </div>
</div>

<table class="table table-hover" style="width:100%">
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                @obj.Message
            </tr>
        }
    </tbody>
</table>